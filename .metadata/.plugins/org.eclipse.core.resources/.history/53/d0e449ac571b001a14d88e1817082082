package com.shops;


import java.sql.Date;

import javax.faces.bean.ManagedBean;

@ManagedBean
public class Store {

	int id;
	String name;
	java.sql.Date founded;
	
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public java.sql.Date getFounded() {
		return founded;
	}
	public void setFounded(Date founded) {
		java.sql.Date sd = new java.sql.Date(founded.getTime());		
		this.founded = sd;
	}
	
	
	
}
