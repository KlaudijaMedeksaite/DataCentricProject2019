package com.shops;
import java.util.Date;

import javax.faces.bean.ManagedBean;

@ManagedBean
public class Store {

	int id;
	String name;
	Date Founded;
	
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public java.sql.Date getFounded() {
		return (java.sql.Date) Founded;
	}
	public void setFounded(java.sql.Date founded) {
		Founded = founded;
	}
	
	
	
}
